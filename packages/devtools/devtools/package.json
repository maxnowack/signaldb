{
  "name": "@signaldb/devtools",
  "version": "1.0.0-beta.4",
  "description": "Developer tools for SignalDB",
  "scripts": {
    "build": "rimraf dist && vite build",
    "analyze-bundle": "bundle-analyzer ./dist --upload-token=$BUNDLE_ANALYZER_UPLOAD_TOKEN --bundle-name=@signaldb/devtools",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxnowack/signaldb.git"
  },
  "homepage": "https://signaldb.js.org",
  "keywords": [
    "developer-tools",
    "devtools",
    "client-database",
    "client",
    "database",
    "hook",
    "local-database",
    "meteor",
    "minimongo",
    "mongodb",
    "offline-first",
    "optimistic-ui",
    "react-hook",
    "react",
    "reactive",
    "reactivity",
    "replication",
    "synchronization",
    "tools",
    "tracker",
    "typescript"
  ],
  "author": "Max Nowack <max.nowack@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js",
      "default": "./dist/index.umd.js"
    }
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@signaldb/core": "^1.1.0"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "styled-components": "^6.1.14"
  },
  "devDependencies": {
    "@types/react": "^19.0.6"
  }
}
